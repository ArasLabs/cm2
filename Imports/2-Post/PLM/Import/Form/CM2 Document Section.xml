      <html_code><![CDATA[<input type="text" name="major_rev" disabled size="1" value="REV" populate="1"/>
<input type="button" name="major_rev_increment" value="+" style="width: 16px" onclick="createNewRevision();" />
<script type="text/javascript">
function createNewRevision()
{
  if(!aras.confirm(aras.getResource("plm", "createnewrevision.confirm")))
  { 
    return;
  }
  var dom = aras.createXMLDocument();
  dom.loadXML(parent.item.xml);
  dom.documentElement.setAttribute('action', 'PE_CreateNewRevision');
	
  var res = aras.soapSend("ApplyMethod", dom.xml);
  if (res.getFaultCode() != 0)
  {
    aras.AlertError(res);
    return;
  }
  res = res.getResultsBody();
  dom.loadXML(res);

  aras.uiReShowItemEx(parent.itemID, dom.documentElement, parent.viewMode);
}
</script>]]></html_code>
